define(function(require, exports, module) {

    'use strict';

    module.name = 'widget-amb-login';

    var bootstrap = require('bootstrap-isobar');
    var base = require('base');
    var core = require('core');
    var ui = require('ui');
    var tac = require('module-tac');
    var tnc = require('module-tnc');
    var crypto = require('module-crypto');
    var interceptor = require('module-amb-interceptor');
    var loading = require('module-amb-loading');
    var analytics = require('module-amb-analytics');

    var deps = [
        core.name,
        ui.name,
        tac.name,
        tnc.name,
        crypto.name,
        interceptor.name,
        loading.name,
        analytics.name
    ];

    // @ngInject
    function run() {
        // Module is Bootstrapped
    }

    module.exports = base.createModule(module.name, deps)
        .controller(require('./controllers'))
        .controller(require('./reactivationControllers'))
        .controller(require('./quickAccessControllers'))
        .service(require('./login-service'))
        .service(require('./reactivation-service'))
        .service(require('./quick-access-service'))
        .directive(require('./am-dir-focus'))
        .run(run);
});
