  		   (new ModalDialogObjects()).focusInputText ();
		 });
	}
	/******************************** GENERATE FORM POP UP ************************************************/
	var _strNavi = ""	
	var _strModal = ""
	for(var i=0; i< _datalength ; i++){
		_strNavi +=  "<a data-toggle=\"modal\" onclick=\"openHref('" + _data[i]["modal-href"]  + "')\" href=\"#\" class=\"btn red\" id=\"d" + i + "\"><div class=\"visual\">"+
