			break;
    
		default : // without assist button
			_data = _datajson["conf-pbb"];
			_datalength = _data.length - 1;
			_alertjson= _alert["alert-pbb"];
			_dataPopUp = _popup.config()["popup-pbb"];
			break;
	}
