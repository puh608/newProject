function getCurrentURL() {              
  var url = "";
  try
  {
    url = (window.parent == null || window.parent == undefined ? window.location.href : window.parent.location.href);
  } 
  catch(ex)
  {
    url = window.location.href;
  }
  if(url == null)
  {
    url = window.location.href;
  }
  return url.toLowerCase();                                                                                          
}                                                                                                                    

