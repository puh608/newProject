function getQueryStringParameter(name)
 {                                    
	name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
  	var regexS = "[\\?&]" + name + "=([^&#]*)";               
  	var regex = new RegExp(regexS);                           
  	var results = regex.exec(document.URL);                   	

  	if(results == null)                                       
    	return "";                                              
  	else                                                      
    	return decodeURIComponent(results[1].replace(/\+/g, " "));
}                                                               

